@using MudBlazor

<MudDialog>
    <DialogContent>
        Form para cadastrar novo cliente.
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">Cancelar</MudButton>
        <MudButton OnClick="Submit">Enviar</MudButton>
    </DialogActions>
</MudDialog>

@code {
    [CascadingParameter]
    public MudDialogInstance? ModalInstance { get; set; }

    private void Submit()
    {
        ModalInstance.Close();
    }
    private void Cancel()
    {
        ModalInstance.Close();
    }
}

