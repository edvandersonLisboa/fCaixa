@using MudBlazor

<MudDialog>
    <DialogContent>
        Tem certeza que quer cancelar compar?
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">Finalizar</MudButton>
        <MudButton OnClick="Submit">Cancelar</MudButton>
    </DialogActions>
</MudDialog>

@code {
    [CascadingParameter]
    public MudDialogInstance? ModalInstance { get; set; }

    private void Submit()
    {
        ModalInstance.Close();
    }
    private void Cancel()
    {
        ModalInstance.Close();
    }
}

